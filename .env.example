# Copy this file to .env.local and fill in your values.
# Example: cp .env.example .env.local

# Supabase project URL (Project Settings -> API)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co

# Supabase client key for browser usage:
# - Preferred: Publishable key (starts with sb_publishable_)
# - Also works: legacy anon key (JWT)
# Never use service_role in NEXT_PUBLIC variables.
NEXT_PUBLIC_SUPABASE_ANON_KEY=sb_publishable_your_key_here

# Server-only admin/API key (Project Settings -> API -> service_role)
# Keep this secret. Do not expose it in client code.
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# false (default): normal app store links in TikTok browser
# true: enable TikTok-specific redirect fallback flow
NEXT_PUBLIC_ENABLE_TIKTOK_REDIRECT_FLOW=false

# false (default): use actual TikTok browser detection
# true: force TikTok browser behavior for local testing
NEXT_PUBLIC_FORCE_TIKTOK_BROWSER=false
